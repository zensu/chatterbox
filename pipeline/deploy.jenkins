#!groovy

properties([disableConcurrentBuilds()])


pipeline {
  agent {
    label 'master'
    }
  options {
    buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
    timestamps()
    }
  stages {
    stage('Stage 1') {
      steps {
        dir('docker') {
	  sh 'docker build -f Dockerfile_base ../'
	}	
      }
    }
    stage('Stage 2') {
      steps {
        dir('docker') {
          sh 'docker build -f Dockerfile ../'
        }
      }
    }
  }
}
